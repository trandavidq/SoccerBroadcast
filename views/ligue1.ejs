<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">

    <title>Document</title>
</head>
<style>
    h1 {text-align: center;}
</style>
<body>
    <a href="/">
        <button type="button" class="btn btn-primary">Home</button>
    </a>
    <a href="/epl">
        <button type="button" class="btn btn-primary">English Premier League</button>
    </a>
    <a href="/bundesliga">
        <button type="button" class="btn btn-primary">Bundeliga</button>
    </a>
    <a href="/laliga">
        <button type="button" class="btn btn-primary">La Liga BBVA</button>
    </a>
    <a href="/seriea">
        <button type="button" class="btn btn-primary">Serie A</button>
    </a>
    <h1>Ligue 1 Standings</h1>
    <table  class="table">
        <tr>
            <th>Position</th>
            <th>Team</th>
            <th >Games Played</th>
            <th>Wins</th>
            <th>Draws</th>
            <th>Losses</th>
            <th>Goal Differential</th>
            <th>Points</th>
        </tr>
        <% for (var i=0;i<theData.length;i++) { %>
            <tr>
                    <td><%= theData[i].position %></td>
                    <td><%= theData[i].team_name %></td>
                    <td><%= theData[i].games_played %></td>
                    <td><%= theData[i].wins %></td>
                    <td><%= theData[i].draws %></td>
                    <td><%= theData[i].losses %></td>
                    <td><%= theData[i].goal_differential %></td>
                    <td><%= theData[i].points %></td>
            </tr>
          <% } %>

        
    </table>
    
    
    
</body>
</html>